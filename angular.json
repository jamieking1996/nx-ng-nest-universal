{
  "version": 1,
  "projects": {
    "ng-test-app": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/ng-test-app",
      "sourceRoot": "apps/ng-test-app/src",
      "prefix": "nx-ng-nest-universal",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ng-test-app/browser",
            "index": "apps/ng-test-app/src/index.html",
            "main": "apps/ng-test-app/src/main.ts",
            "polyfills": "apps/ng-test-app/src/polyfills.ts",
            "tsConfig": "apps/ng-test-app/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/ng-test-app/src/favicon.ico",
              "apps/ng-test-app/src/assets"
            ],
            "styles": [
              "apps/ng-test-app/src/styles.scss"
            ],
            "scripts": ["node_modules/tincanjs/build/tincan-min.js"]
          },
          "configurations": {
            "production": {
              "deployUrl": "/dev/",
              "baseHref": "/dev/",
              "fileReplacements": [
                {
                  "replace": "apps/ng-test-app/src/environments/environment.ts",
                  "with": "apps/ng-test-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "hr": {
              "outputPath": "dist/ng-test-app/browser/hr",
              "deployUrl": "/hr/",
              "baseHref": "/hr/",
              "aot": true,
              "i18nFile": "apps/ng-test-app/messages.hr.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "hr",
              "i18nMissingTranslation": "error"
            },
            "hr-prod": {
              "fileReplacements": [
                {
                  "replace": "apps/ng-test-app/src/environments/environment.ts",
                  "with": "apps/ng-test-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "outputPath": "dist/ng-test-app/browser/hr",
              "deployUrl": "/dev/hr/",
              "baseHref": "/dev/hr/",
              "aot": true,
              "i18nFile": "apps/ng-test-app/messages.hr.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "hr",
              "i18nMissingTranslation": "error"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ng-test-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ng-test-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-test-app:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/ng-test-app/tsconfig.app.json",
              "apps/ng-test-app/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/ng-test-app/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/ng-test-app/jest.config.js",
            "tsConfig": "apps/ng-test-app/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/ng-test-app/src/test-setup.ts"
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/ng-test-app/server",
            "main": "apps/ng-test-app/server.ts",
            "tsConfig": "apps/ng-test-app/tsconfig.server.json",
            "externalDependencies": [
              "@nestjs/microservices",
              "@nestjs/microservices/microservices-module",
              "@nestjs/websockets",
              "@nestjs/websockets/socket-module",
              "cache-manager"
            ]
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "apps/ng-test-app/src/environments/environment.ts",
                  "with": "apps/ng-test-app/src/environments/environment.prod.ts"
                }
              ],
              "sourceMap": false,
              "optimization": false
            },
            "hr": {
              "i18nFile": "apps/ng-test-app/messages.hr.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "hr",
              "i18nMissingTranslation": "error"
            },
            "hr-prod": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "apps/ng-test-app/src/environments/environment.ts",
                  "with": "apps/ng-test-app/src/environments/environment.prod.ts"
                }
              ],
              "sourceMap": false,
              "optimization": false,
              "i18nFile": "apps/ng-test-app/messages.hr.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "hr",
              "i18nMissingTranslation": "error"
            }
          }
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "options": {
            "browserTarget": "ng-test-app:build",
            "serverTarget": "ng-test-app:server"
          },
          "configurations": {
            "production": {
              "browserTarget": "ng-test-app:build:production",
              "serverTarget": "ng-test-app:server:production"
            },
            "hr": {
              "browserTarget": "ng-test-app:build:hr",
              "serverTarget": "ng-test-app:server:hr"
            }
          }
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "browserTarget": "ng-test-app:build:production",
            "serverTarget": "ng-test-app:server:production",
            "routes": [
              "/"
            ]
          },
          "configurations": {
            "production": {}
          }
        }
      }
    },
    "ng-test-app-e2e": {
      "root": "apps/ng-test-app-e2e",
      "sourceRoot": "apps/ng-test-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ng-test-app-e2e/cypress.json",
            "tsConfig": "apps/ng-test-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "ng-test-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ng-test-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/ng-test-app-e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/ng-test-app-e2e/**"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "analytics": false
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "ng-test-app"
}